{
    "version": 2,
    "name": "marvinjs",
    "regions": ["bru"],
    "builds": [
        {
            "src": "package.json",
            "use": "@now/next"
        },
        {
            "src": "src/index.js",
            "use": "@now/node-server"
        }
    ],
    "env": {
        "NODE_PATH": "./src",
        "SLACK_TOKEN": "@marvinjs-slack-token",
        "DATABASE_URL": "@marvinjs-database-url"
    }
}